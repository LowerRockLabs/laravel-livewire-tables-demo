{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "guzzlehttp/guzzle": "^7.0.1",
        "laravel/framework": "^10.0",
        "laravel/pint": "^1.6",
        "maatwebsite/excel": "^3.1",
        "livewire/livewire": "^2.12",
        "spatie/laravel-package-tools": "^1.14"
    },
    "require-dev": {
        "ext-sqlite3": "*",
        "barryvdh/laravel-debugbar": "^3.6",
        "brianium/paratest": "^7.1",
        "fakerphp/faker": "^1.9.1",
        "laravel/dusk": "^7.7",
        "laravel/pint": "^1.6",
        "laravel/sail": "^1.21",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^7.0",
        "nunomaduro/larastan": "^2.2",
        "orchestra/testbench": "^6.13|^7.0|^8.0",
        "pcov/clobber": "^2.0",
        "phpstan/phpstan": "^1.8",
        "phpunit/phpunit": "^10.0",
        "roave/security-advisories": "dev-latest",
        "spatie/laravel-ignition": "^2.0",
        "spatie/laravel-ray": "^1.9"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/",
            "Rappasoft\\LaravelLivewireTables\\": "vendor/rappasoft/laravel-livewire-tables/src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rappasoft\\LaravelLivewireTables\\Tests\\": "vendor/rappasoft/laravel-livewire-tables/tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "clear-all": [
            "@php artisan clear-compiled",
            "@php artisan cache:clear",
            "@php artisan route:clear",
            "@php artisan view:clear",
            "@php artisan config:clear",
            "composer dumpautoload -o"
        ],
        "cache-all": [
            "@php artisan config:cache",
            "@php artisan route:cache"
        ],
        "reset": [
            "composer clear-all",
            "composer cache-all"
        ],
        "run-paratest": [
            "@php -d pcov.enabled=1 vendor/bin/paratest --passthru-php=\"'-d pcov.enabled=1' '-d pcov.directory=vendor/rappasoft/laravel-livewire-tables/src/'\""
        ],
        "initial-setup": [
            "composer update",
            "touch database/database.sqlite",
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\"",
            "@php artisan key:generate",
            "@php artisan migrate:fresh --seed",
            "npm update",
            "npm i alpinejs",
            "npm i @alpinejs/focus",
            "npm i @nextapps-be/livewire-sortablejs",
            "npm i @fontsource/inter",
            "npm update",
            "npm run production",
            "@php artisan view:clear",
            "@php artisan clear-compiled",
            "@php artisan cache:clear",
            "@php artisan route:clear",
            "@php artisan view:clear",
            "@php artisan config:clear",
            "@php artisan config:cache",
            "@php artisan route:cache",
            "chown -R www-data:www-data ."
        ],
        "npm-reset": [
            "composer update",
            "npm update",
            "npm run production",
            "php artisan view:clear",
            "chown -R www-data:www-data ."
        ],
        "pint": "./vendor/bin/pint"
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/LowerRockLabs/laravel-livewire-tables.git"
        }
    ]
}